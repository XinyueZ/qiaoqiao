<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <item android:gravity="center">
        <shape android:shape="oval">
            <solid android:color="@color/colorCyan"/>
            <size
                android:width="@dimen/vision_image_size"
                android:height="@dimen/vision_image_size"/>
            <padding android:left="@dimen/circle_padding"
                     android:top="@dimen/circle_padding"
                     android:right="@dimen/circle_padding"
                     android:bottom="@dimen/circle_padding" />

        </shape>
    </item>
</layer-list>


