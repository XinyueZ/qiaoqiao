<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools">

    <data class="WebViewBinding">

        <variable
            name="visionEntity"
            type="com.qiaoqiao.vision.model.VisionEntity"/>

        <variable
            name="viewholder"
            type="com.qiaoqiao.vision.ui.VisionListAdapter.WebViewHolder"/>
    </data>

    <FrameLayout
        style="@style/VisionItemRoot"
        android:background="@drawable/selector_round_blue_rectangle_item_background"
        android:onClick="@{() -> viewholder.onClicked(visionEntity)}">


        <TextView
            android:id="@+id/vision_tv"
            style="@style/VisionItemTextView"
            android:drawableEnd="@drawable/ic_web_link_open"
            android:drawableRight="@drawable/ic_web_link_open"
            tools:text="hello,world"/>

    </FrameLayout>
</layout>