<?xml version="1.0" encoding="utf-8"?>
<android.support.v7.preference.PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android"
    >
    <android.support.v7.preference.ListPreference
        android:entries="@array/preference_camera_flash_options_entries"
        android:entryValues="@array/preference_camera_flash_options_values"
        android:key="@string/preference_key_camera_flash_options"
        android:summary="@string/preference_summary_camera_flash_options"
        android:title="@string/preference_title_camera_flash_options"
        android:defaultValue="3"
        />
    <android.support.v7.preference.CheckBoxPreference
        android:defaultValue="true"
        android:key="@string/preference_key_camera_auto_focus_allowed"
        android:summary="@string/preference_summary_camera_auto_focus_allowed"
        android:title="@string/preference_title_camera_auto_focus_allowed"
        />


</android.support.v7.preference.PreferenceScreen>